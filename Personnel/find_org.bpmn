    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="SequenceFlow_1kcq14o" name="No" sourceRef="ExclusiveGateway_0b5fzj1" targetRef="Activity_1pnb21r" />
    <bpmn:sequenceFlow id="Flow_1enbumf" sourceRef="Activity_15ergve" targetRef="EndEvent_01vkwbp" />
    <bpmn:scriptTask id="Activity_15ergve" name="Set Org">
      <bpmn:incoming>Flow_0ach5oy</bpmn:incoming>
      <bpmn:incoming>Flow_0qwr6st</bpmn:incoming>
      <bpmn:outgoing>Flow_1enbumf</bpmn:outgoing>
      <bpmn:script># global org
# global org_code
if len(enum_variations) == 1:
    org = enum_variations[0].label
    org_code = enum_variations[0].value
else:
    org = Variation.label
    org_code = Variation.value</bpmn:script>
